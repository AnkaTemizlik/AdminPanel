        "UpdateQuery": {
          "Lines": [
            "UPDATE {TablePrefix}Appointment ",
            "SET {Fields}",
            "WHERE Id = @Id"
          ]
        },
        "DeleteQuery": {
          "Lines": [
            "SELECT s.Id AS SmsId ",
            "INTO #ids ",
            "FROM PA_Sms s ",
            "INNER JOIN PA_AppointmentSms a ",
            "ON a.SmsId = s.Id ",
            "WHERE a.AppointmentId = @Id AND s.Sent = 0",
            "",
            "DELETE s FROM PA_Sms s INNER JOIN #ids a ON a.SmsId = s.Id ",
            "DELETE a FROM PA_AppointmentSms a INNER JOIN #ids s ON s.SmsId = a.SmsId ",
            "",
            "DELETE FROM {TablePrefix}Appointment WHERE Id = @Id"
          ]
        }
