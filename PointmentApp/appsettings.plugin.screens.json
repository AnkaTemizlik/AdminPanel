    "CardQueries": {
      "StateSummary": {
        "Lines": [
          "SELECT a.[State], COUNT(*) AS [Count]",
          "FROM [dbo].[PA_Appointment] a",
          "WHERE [State] != 10",
          "GROUP BY a.[State]"
        ]
      },
      "DateSummary": {
        "Lines": [
          "SELECT CONVERT(varchar, StartDate, 7) AS StartDate, COUNT(*) AS [Count]",
          "FROM [dbo].[PA_Appointment] a",
          "WHERE StartDate > CAST(CAST(GETDATE() AS DATE) AS DATETIME) - 30",
          "GROUP BY CONVERT(varchar, StartDate, 7)"
        ]
