      "Sms": {
        "InsertQuery": {
          "Lines": [
            "RAISERROR('Yalnızca randevu değişikliklerinde otomatik olarak SMS eklenir. ', 16, 1)"
          ]
        },
        "UpdateQuery": {
          "Lines": [
            "IF EXISTS(",
            "  SELECT 0 FROM {TablePrefix}Sms s",
            "  WHERE s.Id = @Id AND [Sent] = 1)",
            "    RAISERROR('Gönderilmiş bir mesajı değiştiremezsiniz.', 16, 1)",
            "ELSE ",
            "  UPDATE s",
            "  SET {Fields}",
            "  FROM {TablePrefix}Sms s",
            "  WHERE s.Id = @Id "
          ]
        },
        "DeleteQuery": {
          "Lines": [
            "IF EXISTS(",
            "  SELECT 0 FROM {TablePrefix}Sms s",
            "  WHERE s.Id = @Id AND [Sent] = 1)",
            "    RAISERROR('Gönderilmiş bir mesajı silemezsiniz.', 16, 1)",
            "ELSE ",
            "BEGIN",
            "  DELETE FROM {TablePrefix}AppointmentSms ",
            "  WHERE SmsId = @Id",
            "",
            "  DELETE s",
            "  FROM {TablePrefix}Sms s",
            "  WHERE s.Id = @Id ",
            "END"
          ]
        },
        "SelectQuery": {
          "Lines": [
            "SELECT a.AppointmentId, a.SmsId, s.[Id], s.[CreationTime], s.[UpdateTime], s.[PhoneNumber], s.[Message], s.[Sender], s.[ScheduledSendingTime], s.[Sent], s.[Response]",
            "FROM {TablePrefix}AppointmentSms a",
            "INNER JOIN {TablePrefix}Sms s ON a.SmsId = s.Id"
          ]
        },
        "SelectById": {
          "Lines": [
            "SELECT a.AppointmentId, a.SmsId, s.[Id], s.[CreationTime], s.[UpdateTime], s.[PhoneNumber], s.[Message], s.[Sender], s.[ScheduledSendingTime], s.[Sent], s.[Response]",
            "FROM {TablePrefix}AppointmentSms a",
            "INNER JOIN {TablePrefix}Sms s ON a.SmsId = s.Id",
            "WHERE a.Id = @Id"
          ]
        }
      },
